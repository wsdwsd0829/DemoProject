source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
use_frameworks!

def shared_pods
    pod 'SVProgressHUD', '~> 2.0' # Progress HUD
    pod 'SDWebImage', '~>3.8' 
    pod 'AFNetworking', '~> 3.0'
end

target 'DoorDashProject' do
    shared_pods
end

target 'DoorDashProjectTests' do
    shared_pods
end
